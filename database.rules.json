{
    "rules": {
        ".read": "auth != null",
        ".write": "auth != null",
        "users": {
            "$userId": {
                "drawings": {
                    "$drawingId": {
                        "layouts": {
                            "layoutId": {
                                "dots": {
                                    ".indexOn": ["index"]
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
